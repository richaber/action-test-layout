{
  "name": "richaber/action-test",
  "type": "wordpress-theme",
  "description": "Testing Github Actions",
  "keywords": [
    "WordPress",
    "Themes"
  ],
  "license": "GPL-2.0-or-later",
  "config": {
    "platform": {
      "php": "8.0"
    },
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
    "php-parallel-lint/php-parallel-lint": "^1.3",
    "phpcompatibility/phpcompatibility-wp": "^2.1",
    "squizlabs/php_codesniffer": "^3.6",
    "staabm/annotate-pull-request-from-checkstyle": "^1.8",
    "wp-cli/i18n-command": "^2.3",
    "wp-coding-standards/wpcs": "^2.3",
    "wptrt/wpthemereview": "^0.2.1"
  },
  "scripts": {
    "phpcs": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcs",
    "parallel-lint": "@php ./vendor/bin/parallel-lint --exclude .git --exclude vendor .",
    "make-pot": "wp i18n make-pot . languages/_s.pot"
  }
}
